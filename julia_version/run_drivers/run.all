#!/bin/bash
#SBATCH --job-name=aw
#SBATCH --nodes=1
#SBATCH --mincpus=16
#SBATCH --time=94:00:00
#SBATCH --output=./complex_nets_2021/log/production-%j.qlog
#SBATCH -p bii
module load julia/1.5.3
module load gurobi/9.0.1

networks=( "fb-pages-politician.edges" "astroph.edges" "wiki.edges" ) 
seedings=( "centola" "random_k_core" )

for network in ${networks[@]}
do
	for seeding in ${seedings[@]}
		do
			sbatch	./run_drivers/driver_jl_mcich_smc_v01.sh "$seeding" "$network"
		done	
done

